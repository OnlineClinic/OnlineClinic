    }

    .chat {
        position: relative;
        background: #f6f4f4;
        border: 1px solid #ebe9e9;
        padding: 10px;
        box-sizing: border-box;
        font-size: 13px;
        border-radius: 5px;
        margin-bottom: 15px;
    }

        .chat:after, .chat:before {
            top: 100%;
            left: 15%;
            border: solid transparent;
            content: " ";
            height: 0;
            width: 0;
            position: absolute;
            pointer-events: none;
        }

        .chat:after {
            border-color: rgba(136, 183, 213, 0);
            border-top-color: #ebe9e9;
            border-width: 10px;
            margin-left: -30px;
        }

