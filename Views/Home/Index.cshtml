@{
    Layout = null;
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html lang="en">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="MyOnlineClinic turns digital devices such as computer, smart-phones and tablets to a very portable medical clinic. Patients and doctors have access to a 24/7 help-desk from anywhere in the world.">
    <meta name="keywords" content="Online visits, online doctor, doctor online, clinic online, see a doctor, see a doctor online, online consultations, video consultations">
    <title>MyOnlineClinic</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/styleindex.css" rel="stylesheet" />
    <script src="~/js/jquery_004.js"></script>
</head>
<body>
    <form method="post" id="form1" style="height:100%;">
        <div id="myCarousel" class="carousel slide">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class=""></li>
                <li class="" data-target="#myCarousel" data-slide-to="1"></li>
                <li class="active" data-target="#myCarousel" data-slide-to="2"></li>
            </ol>
            <!-- Wrapper for Slides -->
            <div class="carousel-inner">
                <div class="item">
                    <!-- Set the first background image using inline CSS below. -->
                    <div class="fill">
                        <img src="~/images/banner3.jpg" alt="#">
                    </div>
                    <!-- <div class="carousel-caption">
                        <img src="images/moc-icon2.png">
                        <h2> I AM </h2>
                    </div> -->
                </div>
                <div class="item">
                    <!-- Set the second background image using inline CSS below. -->
                    <div class="fill">
                        <img src="~/images/banner1.jpg" alt="#">
                    </div>
                    <!-- <div class="carousel-caption">
                        <img src="images/moc-icon2.png">
                        <h2> I AM </h2>
                    </div> -->
                </div>
                <div class="item active">
                    <!-- Set the third background image using inline CSS below. -->
                    <div class="fill">
                        <img src="~/images/banner2.jpg" alt="#">
                    </div>
                    <!-- <div class="carousel-caption">
                        <img src="images/moc-icon2.png">
                        <h2> I AM </h2>
                    </div> -->
                </div>
            </div>
            <!-- Controls -->
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                <span class="icon-prev">
                </span>
            </a><a class="right carousel-control" href="#myCarousel" data-slide="next"><span class="icon-next"></span></a>
        </div>
        <div class="main-div">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <ul class="blog-cricle text-center">
                            <li>
                                <a class="btn_img1" target="blank" href="/patient">
                                    Patients
                                </a> <a class="btn_img" target="blank" href="/doctor">
                                    Healthcare Providers
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!-- jQuery -->
        <script type="text/javascript" src="~/js/jquery_003.js"></script>
        <script type="text/javascript" src="~/js/bootstrap.js"></script>
        <!-- Script to Activate the Carousel -->
        <script type="text/javascript">
            $('.carousel').carousel({
                interval: 5000 //changes the speed
            })
        </script>
        <script src="~/js/jquery.js" type="text/javascript"></script>
        <script src="~/js/jquery_002.js" type="text/javascript"></script>
    </form>
</body>
</html>