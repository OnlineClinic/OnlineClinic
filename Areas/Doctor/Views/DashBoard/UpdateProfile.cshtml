                                                    new AjaxUpdateOptions
                                                    {
                                                        DisableTargetWhenEmpty = true,
                                                        Url = Url.Action("GetSubCategoryInProfession", "Home", new { @area = "" })
                                                    }
                                                    ))



                                                                                </div>
                                                                                <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                                    <label>Profession Sub Category:</label>
                                                                                    @Html.DropDownListFor(model => model.ProfessionCategory, new SelectList(Model.ProfessionSubType, "SubId", "ProfessionSub"), "Select Profession Sub Category", new { @class = "form-control col-md-7 col-xs-12 text_row", required = "required" })

                                                                                </div>
                                                                                <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                                    <label>Interest Area:</label>

                                                                                    @Html.TextBoxFor(x => x.IntrestArea, new { @class = "form-control col-md-7 col-xs-12 text_row", name = "IntrestArea", placeholder = "Area of Interest*", required = "required" })

                                                                                </div>
                                                                                <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                                    <label>Qualification:</label>
                                                                                    @Html.TextBoxFor(x => x.qualification, new { @class = "form-control col-md-7 col-xs-12 text_row", name = "Qualification", placeholder = "Qualification" })
                                                                                </div>
                                                                                <div class=" col-sm-12 col-xs-12 form-group has-feedback">
                                                                                    <label>AHPRA No:</label>

                                                                                    @Html.TextBoxFor(x => x.AHPRANo, new { @class = "form-control col-md-7 col-xs-12 text_row", name = "AHPRANo", placeholder = " AHPRA No.*", required = "required" })

                                                                                </div>
                                                                                <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                                    <label>Video Provider No:</label>

                                                                                    @Html.TextBoxFor(x => x.VideoProviderNo, new { @class = "form-control col-md-7 col-xs-12 text_row", name = "ProviderNo", placeholder = "Provider No. For Video Consult*", required = "required" })

                                                                                </div>

                                                                            </div>
                                                                            <div class="col-md-6" style="padding-right:0px;">

                                                                                <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                                    <label>Home Visit Provider No:</label>

                                                                                    @Html.TextBoxFor(x => x.HomeVisitProviderNo, new { @class = "form-control col-md-7 col-xs-12 text_row", name = "ProviderNo", placeholder = "Provider No. For Home Visit*", required = "required" })

                                                                                </div>


                                                                                <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                                    <label>Prescriber No:</label>
                                                                                    @Html.TextBoxFor(x => x.PrescriberNo, new { @class = "form-control col-md-7 col-xs-12 text_row", name = "PrescriberNo", placeholder = "Prescriber No.*", required = "required" })

                                                                                </div>
                                                                                <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                                    <label>Doctor Profile:</label>

                                                                                    @Html.TextAreaFor(x => x.DoctorProfile, new { @class = "form-control col-md-7 col-xs-12 text_row", name = "DoctorProfile", placeholder = "Doctor Profile.*", required = "required" })

                                                                                </div>
                                                                            </div>
                                                                        </div>


                                                                    </div>



                                                                </div>

                                                            </fieldset>


                                                            <button type="submit" class="btn btn-success">Submit</button>

                                                        </fieldset>
                                                    }
                                                </div>
                                                <div class="tab-pane fade" id="tab2MyAccount">
                                                    <fieldset style="border:groove" class="fieldset">

                                                        <legend class="prople" style="width: 17%; color: #0aa49c">Account Details</legend>

                                                        <div class="x_content">
                                                            <div class="row">
                                                                <div class="col-sm-4">



                                                                    <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                        <label>Bank Name</label>
                                                                        <input id="txtBankmae" class="form-control col-md-7 col-xs-12 text_row" type="text" name="txtBankmae" />
                                                                    </div>


                                                                    <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                        <label>Account Name</label>
                                                                        <input id="txtaccount" class="form-control col-md-7 col-xs-12 text_row" type="text" name="txtaccount" />

                                                                    </div>
