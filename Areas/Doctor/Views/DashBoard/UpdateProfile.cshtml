</div>
<script type="text/javascript">
    $(document).ready(function () {

        $('#DOB').datepick({ dateFormat: 'dd/mm/yyyy' });



    });

    $('#fee').click(function () {

        $.ajax({
            type: "POST",
            url: '/doctor/DashBoard/ConsultFee',
            data: {

                TMAStatdardFee: $('#tmastandardFee').val(),
                TMALongFee: $('#tmalongfee').val(),
                TMAVeryLongFee: $('#tmaverylong').val(),
                TMACurrencyType: $('#TMACurrencyType').val(),
                ClinicLongFee: $('#clinicfee').val(),
                ClinicVeryLongFee: $('#cliniclong').val(),
                tmaRepeatPrescription: $('#tmaRepeate').val(),
                MemberId: $('#MemberId').val()
            },
            datatype: "json",
            success: function (data) {
                $('#tmastandardFee').val() = data.TMAStatdardFee,
                 $('#tmalongfee').val() = data.TMALongFee,
               $('#tmaverylong').val() = data.TMAVeryLongFee,
