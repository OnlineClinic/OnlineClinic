                                                        <legend class="prople" style="width: 21%; color: #0aa49c">Consult Fees Details</legend>

                                                        <div class="x_content">
                                                            <div class="row">
                                                                <div class="col-sm-4">
                                                                    <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                        <h4 style="color: darkslategrey;">
                                                                            Telemedicine Fees :
                                                                        </h4>
                                                                    </div>

                                                                    <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                        <label>Currency Code:</label>
                                                                        @Html.DropDownListFor(x => x.TMACurrencyType, new SelectList(ViewBag.CurrencyCode, "Value", "Text"), new { @class = "form-control col-md-7 col-xs-12 text_row", required = "required" })

                                                                    </div>
                                                                    <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                        <label>Statdard Consult Fee:</label>
                                                                        <input id="tmastandardFee" class="form-control col-md-7 col-xs-12 text_row" type="text" name="tmastandardFee" />
                                                                    </div>


                                                                    <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                        <label>Long Consult Fee:</label>
                                                                        <input id="tmalongfee" class="form-control col-md-7 col-xs-12 text_row" type="text" name="tmalongfee" />

                                                                    </div>
                                                                    <div class="col-sm-12 col-xs-12 form-group has-feedback">
                                                                        <label>Very Long Consult Fee:</label>
                                                                        <input id="tmaverylong" class="form-control col-md-7 col-xs-12 text_row" type="text" name="tmaverylong" />
