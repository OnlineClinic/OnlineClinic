    function getCreatedDate(inputDate) {
        var today = new Date(parseInt(inputDate.substr(6)));
        var dd = today.getDate();
        var mm = today.getMonth() + 1; //January is 0!

        var yyyy = today.getFullYear();
        if (dd < 10) {
            dd = '0' + dd
        }
        if (mm < 10) {
            mm = '0' + mm
        }

        var SubmitDate = dd + '/' + mm + '/' + yyyy;
        return SubmitDate;
    }
