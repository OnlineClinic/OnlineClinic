                                    <li><a href="/Admin/dashboard/adduser"><i class="fa fa-circle-o"></i> Add Staff</a></li>
                                }
                                @if (viewStaff)
                                {
                                    <li><a href="/Admin/dashboard/UsersList"><i class="fa fa-circle-o"></i> Staff List</a></li>
                                }
                            </ul>
                        </li>
                    }

