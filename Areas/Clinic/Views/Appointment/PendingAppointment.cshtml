@{
    var usermodel = ViewBag.LoginUserModel as LoginUserViewModel;
}
<section class="content-header" style="margin-top:45px;">
    <h1 class="underline14">
        Pending Appointments
    </h1>
    <ol class="breadcrumb">
        @if (MyOnlineClinic.Web.Helper.PermissionHelper.CheckPermission("Book Appointment", (int)ModuleNames.ManageAppointment, usermodel.LoginId))
        {
            <li style="margin-right: 5px !important;"><a href="#"><button type="button" class="btn btn-success new">Book An Appointment</button></a></li>
        }
    </ol>
</section>
<div class="container" style="width:1086px;">
    <div class="row">
        <div class="col-md-12 fifth layer">
            <div class="col-md-2">
                <input type="text" class="form-control" id="txtOrgname" placeholder="Organisation Name" style="margin-left: -16px; width: 173px;">
            </div>
            <div class="col-md-2">
                <input type="text" class="form-control" id="txtDoctorName" placeholder="Doctor Name" style="margin-left: -16px; width: 173px;">
            </div>
            <div class="col-md-2">
                <input type="text" class="form-control" id="txtpatientname" placeholder="Patient Name" style="margin-left: -15px; width: 173px;">
            </div>
            <div class="col-md-2" style="">
                @Html.DropDownList("AppointmentType", new List<SelectListItem>
                                    {
                                          new SelectListItem{ Text="Appointment Type", Value = "0" },
                                        new SelectListItem{ Text="Video Consult", Value = "1" },
                                        new SelectListItem{ Text="Clinic Visit", Value = "2" },
                                         new SelectListItem{ Text="Home Visit", Value = "3" },
                                     }, new { @class = "form-control select", style = "margin-left: -14px; width: 173px;", required = "required" })
            </div>
            <div class="col-md-2" style="">
                @Html.DropDownList("AppointmentStatus", new List<SelectListItem>
                                    {
                                        new SelectListItem{ Text="Appointment Status", Value = "0" },
                                           new SelectListItem{ Text="Pending", Value = "1" },
                                        new SelectListItem{ Text="Completed", Value = "2" },
                                         new SelectListItem{ Text="Approved", Value = "3" },
                                            new SelectListItem{ Text="Rejected", Value = "6" },
                                             new SelectListItem{ Text="Cancelled By Doctor", Value = "6" },
                                              new SelectListItem{ Text="Cancelled By Patient", Value = "6" },
                                               new SelectListItem{ Text="Approved Not Completed", Value = "6" },
                                                new SelectListItem{ Text="Not Completed", Value = "6" },
                                    }, new { @class = "form-control select", style = "margin-left: -14px; width: 173px;", required = "required" })
            </div>
            <div class="col-md-2">
                <input type="text" class="form-control" id="country" placeholder="Country" style="margin-left: -11px; width: 173px;">
            </div>
            <div class="col-md-2">
                <input type="text" class="form-control" id="state" placeholder="State" style="margin-left: -16px;width: 173px;margin-top: 10px;">
            </div>
            @*<div class="col-md-2">
                    <input type="text" class="form-control" id="inputEmail" placeholder="Post Code" style="margin-left: -16px;width: 173px;margin-top: 10px;">
                </div>*@
            <div class="col-md-2">
                <input type="text" class="form-control" id="FromDate" placeholder="From Date" style="margin-left: -14px;width: 173px;margin-top: 10px;">
            </div>
            <div class="col-md-2">
