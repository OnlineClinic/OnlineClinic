    $('#checksearching').click(function () {
        $.ajax({
            type: "POST",
            url: '/Clinic/Appointment/Search',
            data: {
                DoctorName: $('#txtDoctorName').val(),
                PatientName: $('#txtpatientname').val(),
                OrganizationName: $('#txtOrgname').val(),
                FroMDate: $('#FromDate').val(),
                TOdate: $('#ToDate').val(),
                AppointmentStatus: $('#AppointmentStatus').val(),
                AppointmentType: $('#AppointmentType').val()
            },
            datatype: "json",
            success: function (data) {
                $('.x_content').hide('fast');
                $('#empInfoDetails').html("");
                $('#empInfoDetails').html(data);
            },
            error: function () {
            }
        });
    });
    $('.btnAssign').click(function () {
        var appointmentId = $(this).attr('data-role');
        var ddlallcatedDoctors = $('#ddlAllocatedDoctorList_' + appointmentId)
        var doctorId = ddlallcatedDoctors.val();
        if (doctorId != 0) {
            alert(doctorId);
            ajaxParam.url = '/Clinic/Appointment/AssignDoctorForHomeVisitAppointment';
            ajaxParam.requestType = 'GET';
            ajaxParam.dataType = 'json';
            ajaxParam.data = { AppointmentId: appointmentId, DoctorId: doctorId };
            ajaxParam.successCallbackFunction = AssignDoctor;
            doAjax(ajaxParam);
        }
        else {
            alert("Please select Doctor");
        }
    });
    function AssignDoctor(data) {
        alert("Doctor Assigned")
        window.location.reload();
    }
